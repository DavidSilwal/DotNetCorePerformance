{
  "version": "1.0.0-*",
  "description": "http://dotnext.ru/spb/talks/sitnik/",
  "authors": [ "Adam Sitnik" ],
  "tags": [ ".NET Core, Performance, DotNext" ],

  "compilationOptions": {
    "emitEntryPoint": true,
    "allowUnsafe": true,
    "optimize": true
  },

  "dependencies": {
    "BenchmarkDotNet": "0.9.6-beta",
    "System.Threading.Tasks.Extensions": "4.0.0-rc2-24027"
  },

  "commands": {
    "Benchmarks": "Benchmarks"
  },

  "frameworks": {
    "net46": {
      "compilationOptions": {
        "define": [ "CLASSIC" ]
      },
      "dependencies": {
        "BenchmarkDotNet.Diagnostics.Windows": "0.9.6"
      },
      "frameworkAssemblies": {
        "System.Threading.Tasks": "4.0.10.0"
      }
    },
    "dnx46": {
      "compilationOptions": {
        "define": [ "CLASSIC" ]
      },
      "dependencies": {
        "BenchmarkDotNet.Diagnostics.Windows": "0.9.6"
      },
      "frameworkAssemblies": {
        "System.Threading.Tasks": "4.0.10.0"
      }
    },
    "dnxcore50": {
      "compilationOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-beta-23516",
        "System.Collections": "4.0.11-beta-23516",
        "System.Console": "4.0.0-beta-23516",
        "System.Linq": "4.0.1-beta-23516",
        "System.Threading": "4.0.11-beta-23516"
      }
    }
  }
}
